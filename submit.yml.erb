---
batch_connect:
  template: "basic"

script:
        native:
                - "-N 1"
                - "--partition=<%= slurm_partition %>"
                - "--cpus-per-task"
                - "<%= slot_cnt.to_i * 8 %>"
                - "--mem=<%= slot_cnt.to_i * 32000 %>"
                - "--time=<%= num_hours.to_i * 60 %>"

